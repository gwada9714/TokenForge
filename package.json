    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1",
    "react-hot-toast": "^2.5.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "react-toastify": "^11.0.3",
    "recharts": "^2.15.1",
    "stream-browserify": "^3.0.0",
    "styled-components": "^6.1.8",
    "ua-parser-js": "^1.0.37",
    "underscore": "^1.12.1",
    "validator": "^13.12.0",
    "viem": "^2.23.1",
    "winston": "^3.17.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
