{
  "name": "tokenforge-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && tsc-alias",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "rimraf node_modules/.vite dist",
    "clean:sw": "rimraf dist/sw.js* && rimraf dev-dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:firebase": "vitest src/__tests__/firebase/ --coverage",
    "test:firebase:watch": "vitest src/__tests__/firebase/ --watch",
    "test:e2e": "firebase emulators:exec 'vitest run src/__tests__/e2e/'",
    "test:load": "artillery run artillery.config.yml",
    "test:load:prod": "artillery run artillery.config.yml --environment production",
    "emulators": "firebase emulators:start",
    "emulators:export": "firebase emulators:export ./emulator-data",
    "emulators:import": "firebase emulators:start --import=./emulator-data",
    "test:e2e:ui": "playwright test",
    "test:e2e:debug": "playwright test --debug",
    "lint:csp": "eslint . --ext .ts,.tsx --plugin @tokenforge/eslint-plugin-csp",
    "security:check": "npm run lint:csp && npm run test:e2e && npm audit",
    "deploy": "node scripts/deploy.js",
    "deploy:secure": "node scripts/deploy-secure.js",
    "security-check": "node scripts/security-check.js",
    "prestart": "npm run security-check",
    "prebuild": "npm run security-check",
    "start": "vite",
    "type-check": "tsc --noEmit",
    "postinstall": "del /S /Q .\\src\\*.d.ts",
    "test:auth": "vitest src/**/*.test.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^6.4.1",
    "@mui/material": "^5.14.18",
    "@opentelemetry/api": "^1.7.0",
