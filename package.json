    "@types/validator": "^13.12.2",
    "@types/ws": "^8.5.10",
    "@wagmi/core": "^2.16.4",
    "broadcast-channel": "^7.0.0",
    "buffer": "^6.0.3",
    "chart.js": "^4.4.7",
    "clsx": "^2.1.1",
    "cookie": "^0.7.0",
    "crypto-js": "^4.2.0",
    "esbuild": "^0.25.0",
    "eventemitter3": "^5.0.1",
    "firebase": "^10.11.1",
    "flat": "^5.0.1",
    "framer-motion": "^12.0.6",
