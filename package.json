    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^3.0.5",
    "@web3modal/wagmi": "^5.1.11",
    "artillery": "^2.0.21",
    "autoprefixer": "^10.4.20",
